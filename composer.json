{
	"name": "slamkajs/rbruteforce2",
	"description": "CakePHP 3+ Plugin for Protection Against BruteForce Attacks",
	"type": "cakephp-plugin",
	"keywords": ["cakephp", "brute force", "security", "egcservices"],
	"homepage": "https://github.com/elsongabriel/rBruteForce",
	"license": "MIT",
	"authors": [
		{
			"name": "rrd",
			"homepage": "http://webmania.cc",
			"role": "Author"
		},
		{
			"name": "egcservices",
			"homepage": "http://egcservices.com.br",
			"role": "Developer"
		},
		 {
		    "name": "Justin Slamka",
		    "email": "justin.slamka@gmail.com",
		    "homepage": "http://slamka.work",
		    "role": "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/elsongabriel/rBruteForce/issues",
		"source": "https://github.com/elsongabriel/rBruteForce"

	},
	"require": {
		"cakephp/cakephp": "3.8.*",
		"cakephp/migrations": "^2.0.0",
		"cakephp/plugin-installer": "^1.0"
	},
	"require-dev": {
        "phpunit/phpunit": "^5|^6"
    },
	"autoload": {
		"psr-4": {
			"RBruteForce\\": "src",
			"RBruteForce\\Test\\Fixture\\": "tests\\Fixture"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Cake\\Test\\": "vendor/cakephp/cakephp/tests",
			"RBruteForce\\Test\\": "tests"
		}
	}
}
